<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"></parameter>
    <parameter name="deviceName" value="emulator-5554"></parameter>
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <parameter name="searchData" value="epam"></parameter>
    <test name="Web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="com.epam.ex1.scenarios"/>
        </packages>
    </test>
</suite>