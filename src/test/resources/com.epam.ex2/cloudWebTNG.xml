<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"/>
    <parameter name="platformName" value="Android"/>
    <parameter name="deviceName" value="SAMSUNG SM-G980F"></parameter>
    <parameter name="udid" value="RF8NB0B3TSL"></parameter>
    <parameter name="browserName" value="Chrome"/>
    <parameter name="searchData" value="epam"/>
    <test name="Cloud web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.ex2.scenarios.webMobileTests"/>
        </classes>
    </test>
</suite>