<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web mobile testing Suite">
    <parameter name="appType" value="web"></parameter>
    <parameter name="platformName" value="Android"></parameter>
    <parameter name="browserName" value="Chrome"></parameter>
    <parameter name="deviceName" value="GOOGLE Pixel 4a"></parameter>
    <parameter name="udid" value="0A211JEC222740"></parameter>
    <parameter name="searchData" value="epam"></parameter>
    <test name="Cloud web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="com.epam.ex2.scenarios"/>
        </packages>
    </test>
</suite>